{
  "name": "php-city",
  "description": "City metaphor visualization of PHP projects",
  "authors": [
    "Adrian Huna"
  ],
  "license": "MIT",
  "keywords": [
    "visualization",
    "PHP",
    "project",
    "structure",
    "three.js"
  ],
  "homepage": "https://github.com/adrianhuna/PHPCity",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "jquery": "^3.1.0",
    "bootstrap": "^3.3.7",
    "underscore": "^1.8.3",
    "stats.js": "*",
    "three.js": "0.79.0",
    "OrbitControls": "https://cdn.jsdelivr.net/npm/three@0.79.0/examples/js/controls/OrbitControls.js",
    "bin-packing": "https://github.com/jakesgordon/bin-packing.git",
    "d3": "^3.5.17",
    "font-awesome": "fontawesome#^4.6.3"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "./dist/css/*.min.*",
        "./dist/js/bootstrap.min.js"
      ]
    },
    "OrbitControls": {
      "dependencies": {
        "three.js": "0.79.0"
      }
    },
    "bin-packing": {
      "main": [
        "./js/packer.growing.js"
      ]
    },
    "font-awesome": {
      "main": [
        "./css/font-awesome.css"
      ]
    }
  }
}
